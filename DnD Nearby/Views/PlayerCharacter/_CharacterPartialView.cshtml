@model PlayerCharacter

<h1>@Model.Name</h1>
<h2>@Model.Race</h2>
<h2>@Model.Class</h2>
<h2>Level: @Model.Level</h2>
<h2>Strength: @Model.Attributes[0]</h2>
<h2>Dexterity: @Model.Attributes[1]</h2>
<h2>Constitution: @Model.Attributes[2]</h2>
<h2>Intelligence: @Model.Attributes[3]</h2>
<h2>Wisdom: @Model.Attributes[4]</h2>
<h2>Charisma: @Model.Attributes[5]</h2>